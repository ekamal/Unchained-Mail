<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Unchained Mail{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('css/fonts.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('css/main.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('css/form.css') }}" rel="stylesheet" type="text/css" />
        {% endblock %}
        {% block js %}
            <script src='http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js'></script>
            <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
            <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
            <script type='text/javascript' src="{{ asset('js/offre.js') }}"></script>
            <script type='text/javascript' src="{{ asset('js/campagne.js') }}"></script>
            <script type='text/javascript' src="{{ asset('js/domaine.js') }}"></script>
            <script type='text/javascript' src="{{ asset('js/ip.js') }}"></script>
            <script type='text/javascript' src="{{ asset('js/isp.js') }}"></script>
            <script type='text/javascript' src="{{ asset('js/data.js') }}"></script>
        {% endblock %}
    </head>
    <body>
    <header id="mainHeader">

        <div id="headerOne">
            <div id="headeOneContent">
                <a href=" {{ path("index") }}"><img src="{{ asset('img/logo.png') }}"></a>
            </div>


        </div>

    </header>

    <section id="mainSection">
        {% block mainContent %}
        {% endblock %}
    </section>

    <footer></footer>

    </body>
</html>
